{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSQLemployees",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "AdventureWorks",
						"type": "DatasetReference"
					},
					"name": "source2"
				},
				{
					"dataset": {
						"referenceName": "Product",
						"type": "DatasetReference"
					},
					"name": "source3"
				},
				{
					"dataset": {
						"referenceName": "FinancialSample",
						"type": "DatasetReference"
					},
					"name": "source4"
				},
				{
					"name": "source5"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlDepartments",
						"type": "DatasetReference"
					},
					"name": "source6"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlLocations",
						"type": "DatasetReference"
					},
					"name": "source7"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlRegions",
						"type": "DatasetReference"
					},
					"name": "source8"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlCountries",
						"type": "DatasetReference"
					},
					"name": "source9"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "source10"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable2",
						"type": "DatasetReference"
					},
					"name": "source11"
				},
				{
					"name": "source12"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				},
				{
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "join2"
				},
				{
					"name": "join3"
				},
				{
					"name": "join4"
				},
				{
					"name": "join5"
				},
				{
					"name": "join6"
				},
				{
					"name": "compose1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EMPLOYEE_ID as decimal(6,0),",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          EMAIL as string,",
				"          PHONE_NUMBER as string,",
				"          HIRE_DATE as timestamp,",
				"          JOB_ID as string,",
				"          SALARY as decimal(8,2),",
				"          COMMISSION_PCT as decimal(2,2),",
				"          MANAGER_ID as decimal(6,0),",
				"          DEPARTMENT_ID as decimal(4,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT * FROM HR.EMPLOYEES;\\nSELECT * FROM HR.DEPARTMENTS;\\nSELECT * FROM HR.REGIONS;\\nSELECT * FROM HR.LOCATIONS;\\nSELECT * FROM HR.COUNTRIES;\\nSELECT * FROM HR.JOBS;\\nSELECT * FROM HR.JOB_HISTORY;',",
				"     format: 'query') ~> source1",
				"source(output(",
				"          SalesOrderLineKey as string,",
				"          ResellerKey as string,",
				"          CustomerKey as string,",
				"          ProductKey as string,",
				"          OrderDateKey as string,",
				"          DueDateKey as string,",
				"          ShipDateKey as string,",
				"          SalesTerritoryKey as string,",
				"          {Order Quantity} as string,",
				"          {Unit Price} as string,",
				"          {Extended Amount} as string,",
				"          {Unit Price Discount Pct} as string,",
				"          {Product Standard Cost} as string,",
				"          {Total Product Cost} as string,",
				"          {Sales Amount} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"source(output(",
				"          ProductKey as string,",
				"          SKU as string,",
				"          Product as string,",
				"          {Standard Cost} as string,",
				"          Color as string,",
				"          {List Price} as string,",
				"          Model as string,",
				"          Subcategory as string,",
				"          Category as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source3",
				"source(output(",
				"          Segment as string,",
				"          Country as string,",
				"          Product as string,",
				"          {Discount Band} as string,",
				"          {Units Sold} as string,",
				"          {Manufacturing Price} as string,",
				"          {Sale Price} as string,",
				"          {Gross Sales} as string,",
				"          Discounts as string,",
				"          Sales as string,",
				"          COGS as string,",
				"          Profit as string,",
				"          Date as string,",
				"          {Month Number} as string,",
				"          {Month Name} as string,",
				"          Year as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source4",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source5",
				"source(output(",
				"          DEPARTMENT_ID as decimal(4,0),",
				"          DEPARTMENT_NAME as string,",
				"          MANAGER_ID as decimal(6,0),",
				"          LOCATION_ID as decimal(4,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source6",
				"source(output(",
				"          LOCATION_ID as decimal(4,0),",
				"          STREET_ADDRESS as string,",
				"          POSTAL_CODE as string,",
				"          CITY as string,",
				"          STATE_PROVINCE as string,",
				"          COUNTRY_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source7",
				"source(output(",
				"          REGION_ID as double,",
				"          REGION_NAME as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source8",
				"source(output(",
				"          COUNTRY_ID as string,",
				"          COUNTRY_NAME as string,",
				"          REGION_ID as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source9",
				"source(output(",
				"          JOB_ID as string,",
				"          JOB_TITLE as string,",
				"          MIN_SALARY as decimal(6,0),",
				"          MAX_SALARY as decimal(6,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source10",
				"source(output(",
				"          EMPLOYEE_ID as decimal(6,0),",
				"          START_DATE as timestamp,",
				"          END_DATE as timestamp,",
				"          JOB_ID as string,",
				"          DEPARTMENT_ID as decimal(4,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source11",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source12",
				"compose(composition: 'undefined') ~> compose1@(temporary)",
				"source1, source6 join(source1@DEPARTMENT_ID == source6@DEPARTMENT_ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1, source7 join(source6@LOCATION_ID == source7@LOCATION_ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2, source9 join(source7@COUNTRY_ID == source9@COUNTRY_ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join3",
				"join3, source8 join(source9@REGION_ID == source8@REGION_ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join4",
				"join4, source10 join(source1@JOB_ID == source10@JOB_ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join5",
				"join5, source11 join(source1@JOB_ID == source11@JOB_ID,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join6",
				"join6 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1) ~> sink1",
				"source2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1) ~> sink2"
			]
		}
	}
}